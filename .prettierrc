{
  "singleQuote": true,
  "semi": true,
  "tabWidth": 2,
  "importOrder": [
    "reflect-metadata",
    "<THIRD_PARTY_MODULES>",
    "^@core/(.*)$",
    "^@components/(.*)$",
    "^@hooks/(.*)$",
    "^@modules/(.*)$",
    "^@styles/(.*)$",
    "^@euk-labs/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "decorators-legacy", "jsx"]
}
